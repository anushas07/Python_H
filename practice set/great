def largest(num1,num2,num3):
    if(num1>num2 and num1>num3):
        print(num1)
    elif(num2>num1 and num2>num3):
        print(num2)
    else:
        print(num3)
         

num1 = int(input("Enter number1"))
num2 = int(input("Enter number2"))
num3 = int(input("Enter number3"))
largest(num1,num2,num3)
