msg = input("Enter the message\n")


if("make a lot of money" in msg):
    spam = True
elif("buy now" in msg):
    spam = True
elif("click this" in msg):
    spam = True
elif("subscribe now" in msg):
    spam = True
else:
    spam = False

if(spam):
    print("This text is spam")
else:
    print("This text is not spam")
